{
    "name": "pixi.js",
    "version": "4.0.1",
    "description": "Pixi.js is a fast lightweight 2D library that works across all devices.",
    "author": "Mat Groves",
    "contributors": [
        "Ivan Popelyshev <ivan.popelyshev@gmail.com>",
        "Matt Karl <matt@mattkarl.com>",
        "Chad Engler <chad@pantherdev.com>",
        "Richard Davey <rdavey@gmail.com>"
    ],
    "main": "./src/index.js",
    "homepage": "http://goodboydigital.com/",
    "bugs": "https://github.com/pixijs/pixi.js/issues",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pixijs/pixi.js.git"
    },
    "scripts": {
        "clean": "rimraf bin && mkdirp bin",
        "prestart": "npm run clean",
        "start": "parallelshell \"npm run watch:lint\" \"npm run watch\"",
        "watch": "pixify -n PIXI -o pixi -w",
        "watch:lint": "watch \"jshint --reporter=scripts/reporter.js scripts src test || exit 0\" src",
        "test": "floss --path test/index.js",
        "test:debug": "npm test -- --debug",
        "lint": "jshint --reporter=scripts/reporter.js scripts src test",
        "prebuild": "npm run lint && npm run clean",
        "build": "pixify -n PIXI -o pixi",
        "postbuild": "npm test",
        "docs": "jsdoc -c scripts/jsdoc.conf.json -R README.md",
        "compile-srcaccessibility": "babel src\\accessibility --out-dir src\\accessibility --require babel-polyfill --presets env",
        "compile-srccorerendererswebgl": "babel src\\core\\renderers\\webgl --out-dir src\\core\\renderers\\webgl --require babel-polyfill --presets env",
        "compile-srccorerendererswebglfiltersspriteMask": "babel src\\core\\renderers\\webgl\\filters\\spriteMask --out-dir src\\core\\renderers\\webgl\\filters\\spriteMask --require babel-polyfill --presets env",
        "compile-srccoreutils": "babel src\\core\\utils --out-dir src\\core\\utils --require babel-polyfill --presets env",
        "compile-srccoregraphicscanvas": "babel src\\core\\graphics\\canvas --out-dir src\\core\\graphics\\canvas --require babel-polyfill --presets env",
        "compile-srcfiltersdisplacement": "babel src\\filters\\displacement --out-dir src\\filters\\displacement --require babel-polyfill --presets env",
        "compile-srcpolyfill": "babel src\\polyfill --out-dir src\\polyfill --require babel-polyfill --presets env",
        "compile-testcore": "babel test\\core --out-dir test\\core --require babel-polyfill --presets env",
        "compile-testinteraction": "babel test\\interaction --out-dir test\\interaction --require babel-polyfill --presets env",
        "compile-srcpreparecanvas": "babel src\\prepare\\canvas --out-dir src\\prepare\\canvas --require babel-polyfill --presets env",
        "compile-srccoredisplay": "babel src\\core\\display --out-dir src\\core\\display --require babel-polyfill --presets env",
        "compile-srccorerendererswebglmanagers": "babel src\\core\\renderers\\webgl\\managers --out-dir src\\core\\renderers\\webgl\\managers --require babel-polyfill --presets env",
        "compile-srccoretextures": "babel src\\core\\textures --out-dir src\\core\\textures --require babel-polyfill --presets env",
        "compile-srcextras": "babel src\\extras --out-dir src\\extras --require babel-polyfill --presets env",
        "compile-srcloaders": "babel src\\loaders --out-dir src\\loaders --require babel-polyfill --presets env",
        "compile-srccorerendererswebglutils": "babel src\\core\\renderers\\webgl\\utils --out-dir src\\core\\renderers\\webgl\\utils --require babel-polyfill --presets env",
        "compile-srcfiltersblur": "babel src\\filters\\blur --out-dir src\\filters\\blur --require babel-polyfill --presets env",
        "compile-srccorespritescanvas": "babel src\\core\\sprites\\canvas --out-dir src\\core\\sprites\\canvas --require babel-polyfill --presets env",
        "compile-srcmesh": "babel src\\mesh --out-dir src\\mesh --require babel-polyfill --presets env",
        "compile-src": "babel src --out-dir src --require babel-polyfill --presets env",
        "compile-srcparticleswebgl": "babel src\\particles\\webgl --out-dir src\\particles\\webgl --require babel-polyfill --presets env",
        "compile-test": "babel test --out-dir test --require babel-polyfill --presets env",
        "compile-srccoremath": "babel src\\core\\math --out-dir src\\core\\math --require babel-polyfill --presets env",
        "compile-srccorerendererswebglfilters": "babel src\\core\\renderers\\webgl\\filters --out-dir src\\core\\renderers\\webgl\\filters --require babel-polyfill --presets env",
        "compile-srcfiltersfxaa": "babel src\\filters\\fxaa --out-dir src\\filters\\fxaa --require babel-polyfill --presets env",
        "compile-srcmeshwebgl": "babel src\\mesh\\webgl --out-dir src\\mesh\\webgl --require babel-polyfill --presets env",
        "compile-srccore": "babel src\\core --out-dir src\\core --require babel-polyfill --presets env",
        "compile-srcinteraction": "babel src\\interaction --out-dir src\\interaction --require babel-polyfill --presets env",
        "compile-srccorespriteswebgl": "babel src\\core\\sprites\\webgl --out-dir src\\core\\sprites\\webgl --require babel-polyfill --presets env",
        "compile-srcextraswebgl": "babel src\\extras\\webgl --out-dir src\\extras\\webgl --require babel-polyfill --presets env",
        "compile-srccoremathshapes": "babel src\\core\\math\\shapes --out-dir src\\core\\math\\shapes --require babel-polyfill --presets env",
        "compile-srcfiltersnoise": "babel src\\filters\\noise --out-dir src\\filters\\noise --require babel-polyfill --presets env",
        "compile-srcextract": "babel src\\extract --out-dir src\\extract --require babel-polyfill --presets env",
        "compile-srccorerendererscanvasutils": "babel src\\core\\renderers\\canvas\\utils --out-dir src\\core\\renderers\\canvas\\utils --require babel-polyfill --presets env",
        "compile-srccorerenderers": "babel src\\core\\renderers --out-dir src\\core\\renderers --require babel-polyfill --presets env",
        "compile-srccoregraphicswebgl": "babel src\\core\\graphics\\webgl --out-dir src\\core\\graphics\\webgl --require babel-polyfill --presets env",
        "compile-srccoregraphics": "babel src\\core\\graphics --out-dir src\\core\\graphics --require babel-polyfill --presets env",
        "compile-srccoresprites": "babel src\\core\\sprites --out-dir src\\core\\sprites --require babel-polyfill --presets env",
        "compile-srcfiltersvoid": "babel src\\filters\\void --out-dir src\\filters\\void --require babel-polyfill --presets env",
        "compile-srccoregraphicswebglutils": "babel src\\core\\graphics\\webgl\\utils --out-dir src\\core\\graphics\\webgl\\utils --require babel-polyfill --presets env",
        "compile-srccoreticker": "babel src\\core\\ticker --out-dir src\\core\\ticker --require babel-polyfill --presets env",
        "compile-srccoretext": "babel src\\core\\text --out-dir src\\core\\text --require babel-polyfill --presets env",
        "compile-srccoregraphicswebglshaders": "babel src\\core\\graphics\\webgl\\shaders --out-dir src\\core\\graphics\\webgl\\shaders --require babel-polyfill --presets env",
        "compile-srcextractwebgl": "babel src\\extract\\webgl --out-dir src\\extract\\webgl --require babel-polyfill --presets env",
        "compile-srcfilterscolormatrix": "babel src\\filters\\colormatrix --out-dir src\\filters\\colormatrix --require babel-polyfill --presets env",
        "compile-srcprepare": "babel src\\prepare --out-dir src\\prepare --require babel-polyfill --presets env",
        "compile-srcparticles": "babel src\\particles --out-dir src\\particles --require babel-polyfill --presets env",
        "compile-srccorerendererscanvas": "babel src\\core\\renderers\\canvas --out-dir src\\core\\renderers\\canvas --require babel-polyfill --presets env",
        "compile-srcpreparewebgl": "babel src\\prepare\\webgl --out-dir src\\prepare\\webgl --require babel-polyfill --presets env",
        "compile-srccoregraphicsutils": "babel src\\core\\graphics\\utils --out-dir src\\core\\graphics\\utils --require babel-polyfill --presets env",
        "compile-srcextractcanvas": "babel src\\extract\\canvas --out-dir src\\extract\\canvas --require babel-polyfill --presets env",
        "compile-srcfilters": "babel src\\filters --out-dir src\\filters --require babel-polyfill --presets env",
        "compile-1_script": "npm run compile-srcaccessibility && npm run compile-srccorerendererswebgl && npm run compile-srccorerendererswebglfiltersspriteMask && npm run compile-srccoreutils && npm run compile-srccoregraphicscanvas && npm run compile-srcfiltersdisplacement && npm run compile-srcpolyfill && npm run compile-testcore && npm run compile-testinteraction && npm run compile-srcpreparecanvas && npm run compile-srccoredisplay && npm run compile-srccorerendererswebglmanagers && npm run compile-srccoretextures && npm run compile-srcextras && npm run compile-srcloaders && npm run compile-srccorerendererswebglutils && npm run compile-srcfiltersblur && npm run compile-srccorespritescanvas && npm run compile-srcmesh && npm run compile-src",
        "compile-2_script": "npm run compile-srcparticleswebgl && npm run compile-test && npm run compile-srccoremath && npm run compile-srccorerendererswebglfilters && npm run compile-srcfiltersfxaa && npm run compile-srcmeshwebgl && npm run compile-srccore && npm run compile-srcinteraction && npm run compile-srccorespriteswebgl && npm run compile-srcextraswebgl && npm run compile-srccoremathshapes && npm run compile-srcfiltersnoise && npm run compile-srcextract && npm run compile-srccorerendererscanvasutils && npm run compile-srccorerenderers && npm run compile-srccoregraphicswebgl && npm run compile-srccoregraphics && npm run compile-srccoresprites && npm run compile-srcfiltersvoid && npm run compile-srccoregraphicswebglutils",
        "compile-3_script": "npm run compile-srccoreticker && npm run compile-srccoretext && npm run compile-srccoregraphicswebglshaders && npm run compile-srcextractwebgl && npm run compile-srcfilterscolormatrix && npm run compile-srcprepare && npm run compile-srcparticles && npm run compile-srccorerendererscanvas && npm run compile-srcpreparewebgl && npm run compile-srccoregraphicsutils && npm run compile-srcextractcanvas && npm run compile-srcfilters",
        "compile": "npm run compile-1_script && npm run compile-2_script && npm run compile-3_script"
    },
    "files": [
        "bin/",
        "src/",
        "CONTRIBUTING.md",
        "LICENSE",
        "package.json",
        "README.md"
    ],
    "dependencies": {
        "bit-twiddle": "^1.0.2",
        "earcut": "^2.0.7",
        "eventemitter3": "^1.1.1",
        "glslify": "^5.0.2",
        "ismobilejs": "^0.4.0",
        "object-assign": "^4.0.1",
        "pixi-gl-core": "^1.0.2",
        "resource-loader": "^1.6.4",
        "browserify-versionify": "^1.0.6"
    },
    "devDependencies": {
        "del": "^2.2.0",
        "electron-prebuilt": "^1.3.2",
        "floss": "^0.7.1",
        "jaguarjs-jsdoc": "^1.0.1",
        "jsdoc": "^3.4.0",
        "jshint": "^2.9.2",
        "jshint-stylish": "^2.2.0",
        "jshint-stylish-summary": "^0.1.2",
        "minimist": "^1.2.0",
        "mkdirp": "^0.5.1",
        "parallelshell": "^2.0.0",
        "pixify": "^1.4.0",
        "rimraf": "^2.5.3",
        "watch": "^0.19.1",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-plugin-transform-object-rest-spread": "latest",
        "babel-preset-env": "latest"
    },
    "browserify": {
        "transform": [
            "glslify",
            "browserify-versionify"
        ]
    }
}
